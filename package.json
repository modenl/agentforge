  "description": "A child game time management desktop application",
  "main": "framework/launcher.js",
  "scripts": {
    "start": "node scripts/start-app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "cd tests/e2e && npm test",
    "test:e2e:headed": "cd tests/e2e && npm run test:headed",
    "test:e2e:debug": "cd tests/e2e && npm run test:debug",
    "test:e2e:install": "cd tests/e2e && npm install && npx playwright install",
    "build": "rollup -c --environment BUILD:production",
    "build:rollup": "rollup -c --environment BUILD:production",
    "build:dev": "rollup -c",
    "dev": "cd framework/renderer/svelte && npm run dev",
    "dev:smart": "node scripts/smart-build.js",
    "setup": "npm install && npm run build",
    "debug": "NODE_ENV=development DEBUG=* npm start"
  },
