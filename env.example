# OpenAI Configuration - 敏感信息
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini Configuration - 敏感信息
GOOGLE_API_KEY=your_google_api_key_here

# AI Models Configuration - 可以为不同agent设置不同模型
OPENAI_MODEL=gpt-4o
ORCHESTRATOR_MODEL=gpt-4o
UI_MODEL=gpt-4o  
EDUCATION_MODEL=gpt-4o

# Supabase Configuration (Optional - for cloud sync)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info

# Security Configuration - 敏感信息
ENCRYPTION_KEY=your_32_character_encryption_key_here
SESSION_SECRET=your_session_secret_here
DEFAULT_PARENT_PASSWORD=parent123

# System Monitoring (Windows specific)
ENABLE_SYSTEM_MONITORING=true
MONITOR_INTERVAL_MS=5000

# Rate Limiting
API_RATE_LIMIT_PER_MINUTE=60
MCP_RATE_LIMIT_PER_MINUTE=100

# Development Settings
DEBUG_MODE=false
ENABLE_DEV_TOOLS=true 